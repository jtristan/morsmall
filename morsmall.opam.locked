  "base-threads" {= "base"}
  "base-unix" {= "base"}
  "cppo" {= "1.8.0"}
  "dune" {= "2.9.3"}
  "menhir" {= "20210419"}
  "menhirLib" {= "20210419"}
  "menhirSdk" {= "20210419"}
  "morbig" {= "dev"}
  "ocaml" {= "4.14.2"}
  "ocaml-base-compiler" {= "4.14.2"}
